@model ProjectMVC.ViewModel.UsersViewModel
@using ProjectMVC.Models
@{
    ViewBag.Title = "Submit";
}



<style>

    .login-popup-heading > h4 {
        color: #E85668;
        font-size: 70px;
        line-height: 30px;
    }

    .new_reg_popup, .new_login_popup {
        border-radius: 2px;
        min-height: 332px;
        width: 500px;
        margin: 10px auto;
    }

    .login-popup-btn {
        background: #E85668;
        border: none;
        border-radius: 25px;
        color: #E85668;
        display: block;
        font-size: 18px;
        height: 38px;
        line-height: 28px;
        margin: 20px auto 5px;
        width: 150px;
        -webkit-transition: all 700ms ease;
        -moz-transition: all 700ms ease;
        -ms-transition: all 700ms ease;
        -o-transition: all 700ms ease;
    }

    a {
        color: #E85668;
        font-size: 18px;
    }

    .bgimg {
        background-image: url('data:image/jpeg;base64,');
    }
</style>

<script src="~/Scripts/jquery-3.3.1.js"></script>
<script src="~/Scripts/jquery.validate.js"></script>
<script src="~/Scripts/jquery.validate.unobtrusive.js"></script>
<link href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">
<div class="login-popup-wrap new_login_popup">
    <div class="login-popup-heading text-middel">
        <h4><i class="fa fa-lock" aria-hidden="true"></i> Login </h4>
    </div>


    <div class="jumbotron">

        <form action="Submit" method="post">
            <label for=" ln">UserName:</label> <input id="UserName" name="UserName" type="text" /> <div style="color: red ">@Html.ValidationMessageFor(x => x.user.UserName)</div>
            <br />
            <label for=" ln">Password:</label> <input id="Password" name="Password" type="password" /><div style="color:red">@Html.ValidationMessageFor(x => x.user.Password)</div>
            <br />

            <input id="Submit" type="submit" class="btn btn-danger" value="Enter" />
        </form>

    </div>




